{
  "name": "skalesafe",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "./electron/ElectronMain.tsx",
  "scripts": {
    "both": "npm run dev & npm run preview",
    "dev": "vite",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "test": "npx jest",
    "electron": "electron .",
    "server": "nodemon --watch './**/*.ts' --exec 'ts-node' ./server/server.ts",
    "app": "concurrently --kill-others \"npm run dev\" \"nodemon --watch './**/*.ts' --exec 'ts-node' ./server/server.ts\"",
    "elec": "concurrently --kill-others \"npm run dev\" \"nodemon --watch './**/*.ts' --exec 'ts-node' ./server/server.ts\" \"npm run electron .\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx, client"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.2.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "buffer": "^6.0.3",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cytoscape": "^3.23.0",
    "dotenv": "^16.0.3",
    "electron": "^22.2.0",
    "express": "^4.18.2",
    "jest": "^29.3.1",
    "mongoose": "^6.9.1",
    "node-powershell": "^5.0.1",
    "nodemon": "^2.0.20",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "prom-client": "^14.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.2",
    "redux-persist": "^6.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-modal": "^3.13.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "lint-staged": {
    "client/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --config ./.prettierrc.js --write"
    ],
    "client/**/*.{css,scss,md,html,json}": [
      "prettier --config ./.prettierrc.js --write"
    ]
  }
}
